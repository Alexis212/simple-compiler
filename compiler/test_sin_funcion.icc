sea MAX:entero=10;
sea mut vec[MAX]:entero = [33, 1, 3, 4, 19, 88, 15, 2, 100, 10];
sea mut i, j, k, num:entero;
sea mut facto:entero = 1;
sea mut fibo, fiba: entero;
sea mut nombre:alfabetico;

// Hola
fn principal()
{
        imprimeln("Dame un numero: ");
        leer(num);

        para i en 1..=num
        {
                facto = facto + facto * i;
        }
        imprimeln!("Factorial(", num, ")=", facto);

        // Ordenar vector (bubble sort)
        para i en 0..MAX
        {
                para j en i+1..MAX-1
                {
                        si vec[i] > vec[j]
                        {
                                tmp = vec[i];
                                vec[i] = vec[j];
                                vec[j] = tmp;
                        }
                }
        }

        // Imprimir vector ordenado
        para i en 0..MAX
        {
                imprimeln("Vector[", i, "]=", vec[i]);
        }
        imprimeln!("]");

        // Fibonacci (version iterativa)
        imprimeln("Dame un numero: ");
        leer(num);

        si num == 0
        {
                fibo = 0;
        }

        si num == 1
        {
                fibo = 1;
        }

        fiba = 0;
        fibo = 1;
        mientras num > 1
        {
                tmp = fibo;
                fibo = fiba + fibo;                                 
                fiba = tmp;
        }
        imprimeln!("Fibonacci(", num, ")=", fibo);

        // Comprobar primo
        imprimeln("Dame un numero: ");
        leer(num);

        ciclo
        {
                si (num % i == 0)
                {
                        bPrimo = falso;
                }

                i = i + 1;
        } mientras i < num;

        imprimeln(num);
        si bPrimo
        {
                imprimeln!(" es PRIMO");
        }
        sino {
                imprimeln!(" NO es PRIMO");
        }

        // Final
        imprimeln("Como te llamas? ");
        leer(nombre);

        imprimeln!("Felicidades ", nombre, "! Ya pasaste compiladores!");
}
