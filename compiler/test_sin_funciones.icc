sea MAX : entero = 10;
sea mut vec[MAX]:entero = [33, 1, 3, 4, 19, 88, 15, 2, 100, 10];
sea mut bPrimo : logico = verdadero;
sea mut fibo, facto : entero = 1;
sea mut i, j, num, tmp, fiba : entero;
sea mut nombre : alfabetico;


fn principal()
{
    // Factorial
    imprimeln("Dame un numero: ");
    leer(num);

    para i en 1..=num
    {
        facto = facto * i; 
    }
    imprimeln!("Factorial(", i-1, ") = ", facto);

    // Fibonacci
    imprimeln("Dame un numero: ");
    leer(num);

    i = num;
    si num == 0 { fibo = 0; }
    si num == 1 { fibo = 1; }

    mientras i > 1
    {
        tmp = fibo;
        fibo = fiba + fibo;
        fiba = tmp;
    
        i = i - 1;
    }
    imprimeln!("Fibonacci", "(", num, ") = ", fibo);

    // Bubble Sort
    para i en 0..MAX-1
    {
        para j en i+1..MAX
        {
            si vec[i] > vec[j]
            {
                tmp = vec[i];
                vec[i] = vec[j];
                vec[j] = tmp;
            }
        }
    }

    // Imprimir vector
    para i en 0..MAX
    {
        imprimeln!("Vector[", i, "] = ", vec[i]);
    }

    // Numero Primo
    imprimeln("Dame un numero: ");
    leer(num);

    i = 2;
    ciclo
    {
        si (num % i == 0) { bPrimo = falso; }
        i = i+1;
    }
    mientras i < num;

    imprimeln(num);
    si bPrimo { imprimeln!(" es primo"); }
    sino { imprimeln!(" NO es primo"); }

    // Final
    imprimeln("Dime tu nombre: ");
    leer(nombre);
    imprimeln!("Felicidades ", nombre, "! Ya pasaste compiladores!");
}
