sea mut lados:entero = 2;

fn principal()
{
        si lados == 0
        {
                imprimeln!("Es un circulo");
        }

        sino si lados > 3
        {
                imprimeln!("Es un poligono");
        }

        sino
        {
                imprimeln!("No es una figura");
        }

        imprimeln!("Adios");
}
